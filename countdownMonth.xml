}

		dayNum = document.getElementById('day').getElementsByTagName('h1')[0];
		dayTitle = document.getElementById('day').getElementsByTagName('h2')[0];
    	dayNum2 = document.getElementById('day2').getElementsByTagName('h1')[0];
