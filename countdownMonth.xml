	function update_countdown() {
		remainingSeconds -= .25;
    remainingSeconds2 -=.25;
		var tmpSeconds = remainingSeconds;
    var tmpSeconds2 = remainingSeconds2;

			weeksTime = 86400*7;

			remainingMonths = Math.max(0,Math.floor(tmpSeconds / (weeksTime*4)));
			tmpSeconds -= remainingMonths * 4 * weeksTime;
      monthNum.innerHTML = remainingMonths;
      monthTitle.innerHTML = remainingMonths == 1 ? 'month' : 'months'

      remainingMonths2 = Math.max(0,Math.floor(tmpSeconds2 / (weeksTime*4)));
			tmpSeconds2 -= remainingMonths2 * 4 * weeksTime;
      monthNum2.innerHTML = remainingMonths2;
	monthTitle2.innerHTML = remainingMonths2 == 1 ? 'month' : 'months'

			remainingWeeks = Math.max(0, Math.floor(tmpSeconds / (weeksTime)));
			tmpSeconds -= remainingWeeks * weeksTime;
			weekNum.innerHTML = remainingWeeks;
      weekTitle.innerHTML = remainingWeeks == 1 ? 'week' : 'weeks'

      remainingWeeks2 = Math.max(0, Math.floor(tmpSeconds2 / (weeksTime)));
      tmpSeconds2 -= remainingWeeks2 * weeksTime;
      weekNum2.innerHTML = remainingWeeks2;
      weekTitle2.innerHTML = remainingWeeks2 == 1 ? 'week' : 'weeks'
		
		
		remainingDays = Math.max(0, Math.floor(tmpSeconds / 86400));
		tmpSeconds -= remainingDays * 86400;
		dayNum.innerHTML = remainingDays;
    dayTitle.innerHTML = remainingDays == 1 ? 'day' : 'days'

remainingDays2 = 	Math.max(0, Math.floor(tmpSeconds2 / 86400));
	tmpSeconds2 -= remainingDays2 * 86400;
    	dayNum2.innerHTML = remainingDays2;
    	dayTitle2.innerHTML = remainingDays2 == 1 ? 'day' : 'days'
		
		remainingHours = Math.max(0, Math.floor(tmpSeconds / 3600));
		tmpSeconds -= remainingHours * 3600;
		hourNum.innerHTML = remainingHours;
    hourTitle.innerHTML = remainingHours == 1 ? 'hour' : 'hours'

	remainingHours2 = Math.max(0, Math.floor(tmpSeconds2 / 3600));
		tmpSeconds2 -= remainingHours2 * 3600;
    hourNum2.innerHTML = remainingHours2;
    hourTitle2.innerHTML = remainingHours2 == 1 ? 'hour' : 'hours'
		
		remainingMinutes = Math.max(0, Math.floor(tmpSeconds / 60));
		tmpSeconds -= remainingMinutes * 60;
    minuteNum.innerHTML = remainingMinutes;
    minuteTitle.innerHTML = remainingMinutes == 1 ? 'minute' : 'minutes'

    remainingMinutes2 = Math.max(0, Math.floor(tmpSeconds2 / 60));
		tmpSeconds2 -= remainingMinutes2 * 60;
		minuteNum2.innerHTML = remainingMinutes2;
